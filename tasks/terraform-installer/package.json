{
  "name": "azure-pipelines-tasks-terraform-installer",
  "version": "1.0.0",
  "description": "Azure devops pipeline task to install terraform",
  "main": ".bin/index.js",
  "scripts": {
    "build": "tsc --build",
    "pack": "copyfiles package.json task.json icon.png \".bin/*.js\" .dist && cd .dist && npm install --only=prod",
    "delete": "tfx build tasks delete --task-id 11645770-d18e-11e8-8f5b-1b8b62612b3b",
    "upload": "tfx build tasks upload --task-path .dist",
    "start": "ts-node --require dotenv/config ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charleszipp/azure-pipelines-tasks-terraform.git"
  },
  "keywords": [
    "terraform",
    "azure-devops",
    "vsts"
  ],
  "author": "Charles Zipp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charleszipp/azure-pipelines-tasks-terraform/issues"
  },
  "homepage": "https://github.com/charleszipp/azure-pipelines-tasks-terraform#readme",
  "devDependencies": {
    "@types/node": "^16.4.1",
    "@types/q": "^1.5.5",
    "copyfiles": "^2.4.1",
    "dotenv": "^10.0.0",
    "tfx-cli": "^0.9.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
    "dotenv": "^10.0.0",
    "tfx-cli": "^0.9.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "azure-pipelines-task-lib": "^3.1.5",
    "azure-pipelines-tool-lib": "^1.0.2",
    "node-fetch": "^2.6.1",
    "q": "^1.5.1"
  }
}
